{
	"name": "rtcamp/blank-theme",
	"type": "wordpress-theme",
	"description": "A base theme for WordPress development.",
	"homepage": "https://github.com/rtcamp/blank-theme",
	"license": "MIT",
	"require": {
	  "php": ">=7.4"
	},
	"require-dev": {
	  "automattic/vipwpcs": "^2.3",
	  "dealerdirect/phpcodesniffer-composer-installer": "^0.7",
	  "phpcompatibility/phpcompatibility-wp": "^2.1",
	  "sirbrillig/phpcs-variable-analysis": "^2.8",
	  "wp-coding-standards/wpcs": "^2.3",
	  "wp-cli/i18n-command": "^2.6"
	},
	"config": {
	  "discard-changes": true,
	  "sort-packages": true,
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"scripts": {
	  "phpcbf": "phpcbf",
	  "phpcs": "phpcs",
	  "pot": "wp i18n make-pot . ./languages/blank-theme.pot --domain=blank-theme --package-name=\"Blank Theme\" --skip-theme-json"
	}
  }
